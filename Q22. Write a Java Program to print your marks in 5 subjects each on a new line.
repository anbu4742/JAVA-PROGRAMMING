import java.util.*;
public class Main{
    public static void main(String[] args){ 
        Scanner sc = new Scanner(System.in);
       int a,b,c,d,e;
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        d = sc.nextInt();
        e = sc.nextInt();
        System.out.println("English:" + a + "\n" + "Tamil:" + b + "\n" + "Maths:" + c + "\n" + "Science:" + d + "\n" + "Social:" + e); 
 	}
}
