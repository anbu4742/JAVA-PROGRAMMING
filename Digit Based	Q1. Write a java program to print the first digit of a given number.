import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a = n;
        int count = 0;
        while(n!=0){
            count++;
            n = n/10;
        }
        int d= 1;
        for(int i = 1;i<count;i++){
         d = d*10;
        }
        int e = a/d;
        System.out.println(e);

        
    }
}
