import java.util.*;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        boolean ps = false;
        for(int i=0;i*i<=n;i++){
            if(i*i==n){
                ps = true;
                break;
            }
        }
        if(ps){
            System.out.println("Perfect Square");
        }
        else{
            System.out.println("Not a Perfect Square");
        }
        
     }
}
