import java.util.*;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = sc.nextInt();
        int a = n;
        int b = m;

        while(m!=0){
            int r = n%m;
            n = m;
            m = r;
        }
        int lcm = (a*b)/n;
        System.out.println("LCM of the two numbers:" + lcm);
     }
}
