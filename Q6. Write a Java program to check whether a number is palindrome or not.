import java.util.*;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int b = n;
        int rev = 0;
        while(n!=0){
            int digit = n%10;
            rev = rev*10 +digit;
            n = n/10;
        }
        if(b == rev){
            System.out.println("It is Palindrome");
        }
        else{
            System.out.println("It is not a Palindrome");
        }
        
        
     }
}
