import java.util.*;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sumf = 0;
        for(int i =1;i<=n;i++){
            if(n%i==0){
                sumf = sumf+i;
                System.out.print(sumf);
            }
        }
        
        
     }
}
