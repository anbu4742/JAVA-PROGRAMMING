import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a,b;
        a = sc.nextInt();
        b = sc.nextInt();
        if(a>b){
            System.out.println(a + "is the greatest number");
        }
        else{
            System.out.println(b + " is the greatest number");
        }


    }
}
